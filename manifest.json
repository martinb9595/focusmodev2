{
  "manifest_version": 3,
  "name": "Focus Mode",
  "version": "1.0",
  "description": "A Chrome extension to help you focus.",
  "permissions": [
    "tabs",
    "storage",
    "notifications"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}
